{
	"name": "shining-soul-j",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://ioneone.github.io/shining-soul-j",
	"description": "",
	"scripts": {
		"clean": "rimraf dist/*",
		"tsc": "tsc",
		"assets": "cp -R src/client/index.html public/ dist/client",
		"parcel": "parcel build src/client/main.ts -d dist/client",
		"build": "npm-run-all clean tsc assets parcel",
		"start": "node dist/server/server.js",
		"dev": "nodemon --watch src -e ts --exec npm-run-all build start",
		"predeploy": "npm run build",
		"deploy": "gh-pages -b master -d dist/client"
	},
	"author": "Junhong Wang",
	"license": "MIT",
	"devDependencies": {
		"@types/parcel-bundler": "^1.12.1",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"eslint": "^6.8.0",
		"gh-pages": "^2.2.0",
		"minimist": ">=1.2.2",
		"nodemon": "^2.0.4",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.4",
		"rimraf": "^3.0.2",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"@types/express": "^4.17.6",
		"@types/node": "^14.0.13",
		"@types/socket.io": "^2.1.8",
		"express": "^4.17.1",
		"http": "0.0.1-security",
		"phaser": "^3.23.0",
		"socket.io": "^2.3.0"
	},
	"parcelCleanPaths": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "public",
		"watcherGlob": "**"
	}
}
